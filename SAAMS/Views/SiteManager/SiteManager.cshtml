</section>
<script>
 

    function SiteManager(cityName) {
        var i;
        var x = document.getElementsByClassName("site");
        for (i = 0; i < x.length; i++) {
            x[i].style.display = "none";
        }
        document.getElementById(cityName).style.display = "block";
    }
</script>
